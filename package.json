{
  "name": "@gkoil/gendiff",
  "version": "1.0.1",
  "description": "gendiff will help you find the difference between two [json/yaml/ini] files",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "gendiff": "src/bin/gendiff.js"
  },
  "scripts": {
    "test": "npx -n --experimental-vm-modules jest --no-warnings",
    "test-coverage": "npm test -- --coverage --coverageProvider=v8",
    "test-watch": "npx -n --experimental-vm-modules jest --watch --no-warnings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GKoil/GenDiff.git"
  },
  "publishConfig": { "registry": "https://npm.pkg.github.com/" },
  "keywords": [
    "diff",
    "diff files",
    "json diff",
    "yaml diff",
    "ini diff"
  ],
  "author": "gkoil",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GKoil/GenDiff/issues"
  },
  "homepage": "https://github.com/GKoil/GenDiff#readme",
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "commander": "^9.5.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.3.1"
  }
}
